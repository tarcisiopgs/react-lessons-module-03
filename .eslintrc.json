{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["react-app"],
  "plugins": ["import", "react"],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": "off",
    "react/jsx-one-expression-per-line": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "react/react-in-jsx-scope": "off"
  }
}
